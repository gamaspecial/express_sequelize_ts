  "license": "MIT",
  "private": true,
  "scripts": {
    "watch": "nodemon",
    "db:create": "sequelize db:create --config config/database.js --charset=utf8mb4",
    "db:migrate": "sequelize db:migrate --config config/database.js",
    "db:rollback": "sequelize db:migrate:undo --config config/database.js",
    "generate:openapi-ts": "npx openapi-typescript openapi.yaml --output src/models/openapi/schema.ts"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^5.0.0",
    "express-validator": "^6.12.0",
    "mysql2": "^2.2.5",
    "openapi-typescript": "^4.0.2",
    "sequelize": "^6.5.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/sequelize": "^4.28.9",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
